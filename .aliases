
# ssh
alias pubkey="more ~/.ssh/id_rsa.pub | pbcopy | printf '=> Public key copied to pasteboard.\n'";

# Behat
alias behathelp="bin/behat -di | less -R"
alias sss="selenium-server -p 4444"

# Enable aliases to be sudoâ€™ed
alias sudo='sudo'

# Tools
alias s='open -a "Sublime Text 3"'
alias chat="open weechat"

# MacOSX
alias sleep="pmset sleepnow"
alias f='open -a Finder ./'
spotlight () { mdfind "kMDItemDisplayName == '$@'wc"; }                         # spotlight: Search for a file using MacOS Spotlight's metadata
alias cleanupDS="find . -type f -name '*.DS_Store' -ls -delete"                 # Recursively delete .DS_Store files
alias finderShowHidden='defaults write com.apple.finder ShowAllFiles TRUE'      #   finderShowHidden:   Show hidden files in Finder
alias finderHideHidden='defaults write com.apple.finder ShowAllFiles FALSE'     #   finderHideHidden:   Hide hidden files in Finder
alias cleanupLS="/System/Library/Frameworks/CoreServices.framework/Frameworks/LaunchServices.framework/Support/lsregister -kill -r -domain local -domain system -domain user && killall Finder"     # Clean up LaunchServices to remove duplicates in the "Open With" menu
alias screensaverDesktop='/System/Library/Frameworks/ScreenSaver.framework/Resources/ScreenSaverEngine.app/Contents/MacOS/ScreenSaverEngine -background'                                            # Run a screensaver on the Desktop

# DevTools 
alias memcaches="sh /Volumes/Vault/Dev/Scripts/Dev/VRT/startmemcached.sh"
alias sassr1="sass --watch sites/all/themes/benny/scss/:sites/all/themes/benny/css/"
alias cpwr1="compass watch /www/vrt/radio1/htdocs/sites/all/themes/benny"

# Colored up cat!
# You must install Pygments first - "sudo easy_install Pygments"
alias c='pygmentize -O style=monokai -f console256 -g'

# Terminal
alias cd..='cd ../'                         # Go back 1 directory level (for fast typers)
alias ..='cd ../'                           # Go back 1 directory level
alias ...='cd ../../'                       # Go back 2 directory levels
alias .3='cd ../../../'                     # Go back 3 directory levels
alias .4='cd ../../../../'                  # Go back 4 directory levels
alias .5='cd ../../../../../'               # Go back 5 directory levels
alias .6='cd ../../../../../../'            # Go back 6 directory levels
alias which='type -all'
alias path='echo -e ${PATH//:/\\n}'
alias DT='tee ~/Desktop/terminalOut.txt'    # DT:           Pipe content to file on MacOS Desktop
alias ttop="top -R -F -s 10 -o rsize"       # Recommended 'top' invocation to minimize resources

# Search
alias qfind="find . -name "                 # qfind:    Quickly search for file
ff () { /usr/bin/find . -name "$@" ; }      # ff:       Find file under the current directory
ffs () { /usr/bin/find . -name "$@"'*' ; }  # ffs:      Find file whose name starts with a given string
ffe () { /usr/bin/find . -name '*'"$@" ; }  # ffe:      Find file whose name ends with a given string
alias numFiles='echo $(ls -1 | wc -l)'      # numFiles:     Count of non-hidden files in current dir

# Alias
alias zshconf="s ~/.zshrc"
alias ohmyzsh="s ~/.oh-my-zsh"

# Projects
alias www="cd /www"
alias cdec="cd /www/ec"
alias cdcwt="cd /www/ec/cwt"
alias beta="cd /www/ec/beta"

# drush 
alias db="drush @beta.local"
alias dby="drush @beta.local -y"
alias dbcc="drush @beta.local cc all -y"

# phpcs
alias dcs="phpcs --standard=Drupal"
#alias phpcs="~/.composer/vendor/bin/phpcs"
