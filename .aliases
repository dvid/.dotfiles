# Git
alias gs='git status'
alias ga='git add'
alias gc='git commit -m' # requires to type a commit message
alias gam='git commit --amend -m' # requires to type a commit message
alias gp='git push'
alias gmt='git mergetool -t opendiff'
alias gac="git reset --soft HEAD~1"
alias gacc="git commit -c ORIG_HEAD"
alias grh="git revert HEAD" #If you have already made your commits public, you will want to create a new commit which will "revert" the changes you made in your previous commit (current HEAD).
alias gso="git remote show origin"

# Networking
alias myip='curl ip.appspot.com'                    # myip:         Public facing IP Address
alias netCons='lsof -i'                             # netCons:      Show all open TCP/IP sockets
alias flushDNS='dscacheutil -flushcache'            # flushDNS:     Flush out the DNS Cache
alias lsock='sudo /usr/sbin/lsof -i -P'             # lsock:        Display open sockets
alias lsockU='sudo /usr/sbin/lsof -nP | grep UDP'   # lsockU:       Display only open UDP sockets
alias lsockT='sudo /usr/sbin/lsof -nP | grep TCP'   # lsockT:       Display only open TCP sockets
alias ipInfo0='ipconfig getpacket en0'              # ipInfo0:      Get info on connections for en0
alias ipInfo1='ipconfig getpacket en1'              # ipInfo1:      Get info on connections for en1
alias openPorts='sudo lsof -i | grep LISTEN'        # openPorts:    All listening connections
alias showBlocked='sudo ipfw list'                  # showBlocked:  All ipfw rules inc/ blocked IPs

# Web Dev Conf
alias aedit='sudo edit /etc/httpd/httpd.conf'
alias arestart='sudo apachectl graceful'
alias mysqlstart='mysql.server start'
alias mysqlstop='mysql.server stop'
alias emysql='vi /usr/local/opt/mysql/my.cnf'
alias ehosts='sudo vi /etc/hosts'
alias evhosts="sudo vi /etc/apache2/extra/httpd-vhosts.conf"
alias httplog='tail /var/log/httpd/error_log'
alias apacheLogs="less +F /var/log/apache2/error_log"
alias ephpini="sudo vi /usr/local/etc/php/5.5/php.ini"
